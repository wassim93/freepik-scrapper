    "fast-csv": "^5.0.2",
    "jsdom": "^24.1.0",
    "puppeteer": "^24.8.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.32",
    "nodemon": "^3.1.10",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
